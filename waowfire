-- LINORIA LIBRARY KÜTÜPHANESİ YÜKLEMESİ
-- =========================================
local repo = 'https://raw.githubusercontent.com/violin-suzutsuki/LinoriaLib/main/'
local Library = loadstring(game:HttpGet(repo .. 'Library.lua'))()

local Window = Library:CreateWindow({
    Title = 'WaowHub FireTouch Version',
    Center = true,
    AutoShow = true,
    TabPadding = 10 -- Sekme başlıklarının taşmasını azaltmak için artırıldı
})

-- SEKME SIRALAMASI: FireTouch -> React -> Boosters -> Range -> Misc -> Settings
local FireTouchTab = Window:AddTab('FireTouch')
local ReactTab = Window:AddTab('React')
local SkillBoostersTab = Window:AddTab('Boosters') 
local RangeTab = Window:AddTab('Range') 
local MiscTab = Window:AddTab('Misc') 
local UISettingsTab = Window:AddTab('Settings') 


----------------------------------------
-- FIRETOUCH TAB (TOOLTIPLER KALDIRILDI)
----------------------------------------
local FireTouchGroup = FireTouchTab:AddLeftGroupbox('FireTouch')

local function CreateReachSlider(partName, initialValue)
    -- FireTouch Tooltip'leri kullanıcı isteği üzerine kaldırıldı.
    
    FireTouchGroup:AddSlider(partName:gsub(" ", "") .. "Slider", { 
        Text = partName .. " Studs",
        Default = initialValue or 0,
        Min = 0,
        Max = 30,
        Rounding = 1,
        Suffix = " studs",
        -- Tooltip parametresi kaldırıldı.

        Callback = function(Value)
            local FloorValue = math.floor(Value * 10) / 10
            HandlePartReach(partName, FloorValue)
        end
    })
end

CreateReachSlider("Left Leg")
CreateReachSlider("Right Leg")
CreateReachSlider("Torso")
CreateReachSlider("Head")
CreateReachSlider("GK")


----------------------------------------
-- REACT TAB 
----------------------------------------
local ReactGroup = ReactTab:AddLeftGroupbox('React Enhancements')
-- Madbliss grubu sağda yer alsın diye AddRightGroupbox kullanıldı.
local MadblissGroup = ReactTab:AddRightGroupbox('Madbliss\'s Reacts') 

ReactGroup:AddButton({
    Text = "Better React",
    Func = function()
        local mt = getrawmetatable(game)
        local oldNC = mt.__namecall
        setreadonly(mt, false)
        mt.__namecall = newcclosure(function(self, ...)
            local args = {...}
            if not checkcaller() and getnamecallmethod() == "FireServer" and self == workspace.FE.Scorer.RemoteEvent then
                pcall(function()
                   workspace.FE.Scorer.RemoteEvent1:FireServer(unpack(args))
                    workspace.FE.Scorer.RemoteEvent2:FireServer(unpack(args))
                end)
                return
            end
            return oldNC(self, unpack(args))
         end)
        setreadonly(mt, true)
    end,
    Tooltip = 'Send Remote Hit (x2)' 
})

ReactGroup:AddButton({
    Text = "Better Hit Registration",
    Func = function()
        local mt = getrawmetatable(game)
        local oldNC = mt.__namecall
        local isEnabled = true
        
        if not getrawmetatable(game).__oldNC then
            getrawmetatable(game).__oldNC = oldNC
        end

        local currentOldNC = getrawmetatable(game).__oldNC

        if isEnabled then
            setreadonly(mt, false)
            mt.__namecall = newcclosure(function(self, ...)
                local args = {...}
                if not checkcaller() and getnamecallmethod() == "FireServer" and self == workspace.FE.Scorer.RemoteEvent then
                    for i = 1, 3 do
                        pcall(function()
                            workspace.FE.Scorer.RemoteEvent1:FireServer(unpack(args))
                            workspace.FE.Scorer.RemoteEvent:FireServer(unpack(args))
                        end)
                    end
                    return
                   end
                return currentOldNC(self, unpack(args))
            end)
            setreadonly(mt, true)
        end
    end,
    Tooltip = 'Spam Hit Signal (x3)' 
})

ReactGroup:AddButton({
    Text = "React Killer",
    Func = function()
        local mt = getrawmetatable(game)
        local oldNC = mt.__namecall
        local isEnabled = true
        
        if not getrawmetatable(game).__oldNC then
            getrawmetatable(game).__oldNC = oldNC
        end

        local currentOldNC = getrawmetatable(game).__oldNC

        if isEnabled then
            setreadonly(mt, false)
            mt.__namecall = newcclosure(function(self, ...)
                local args = {...}
                if not checkcaller() and getnamecallmethod() == "FireServer" and self == workspace.FE.Scorer.RemoteEvent then
                    for i = 1, 10 do
                        if workspace:FindFirstChild("FE") then
                            local fe = workspace.FE
                            if fe:FindFirstChild("Keep") and fe.Keep:FindFirstChild("GK") then
                                pcall(function() fe.Keep.GK:FireServer(unpack(args)) end)
                            end
                            if fe:FindFirstChild("GK") then
                                if fe.GK:FindFirstChild("BGKSaves") then pcall(function() fe.GK.BGKSaves:FireServer(unpack(args)) end) end
                                if fe.GK:FindFirstChild("BGKP") then pcall(function() fe.GK.BGKP:FireServer(unpack(args)) end) end
                                if fe.GK:FindFirstChild("GGKP") then pcall(function() fe.GK.GGKP:FireServer(unpack(args)) end) end
                           end
                        end
                    end
                    return
                end
                return currentOldNC(self, unpack(args))
            end)
            setreadonly(mt, true)
        end
    end,
    Tooltip = 'Spam GK React Signals (x10)' 
})

ReactGroup:AddButton({
    Text = "Alz React",
    Func = function()
        pcall(function()
            local tpsPart = workspace.TPSSystem and workspace.TPSSystem:FindFirstChild("TPS")
            if tpsPart then
                tpsPart.Velocity = Vector3.new(100, 100, 100)
            end
        end)
        
        pcall(function()
            local practice = workspace:FindFirstChild("Practice")
            if practice then
                for _, ball in pairs(practice:GetChildren()) do
                    if ball.Name == "PSoccerBall" and ball:IsA("BasePart") then
                        if ball and ball.Parent then
                            ball.Velocity = Vector3.new(100, 100, 100)
                         end
                    end
                end
            end
        end)
    end,
    Tooltip = 'Boost Ball Velocity' 
})

ReactGroup:AddButton({
    Text = "Boost React Detection ( Match Ball )",
    Func = function()
        local tpsPart = workspace.TPSSystem and workspace.TPSSystem:FindFirstChild("TPS")

        if not tpsPart then
            return
        end

        local char = getCharacter()
        local hrp = char and char:FindFirstChild("HumanoidRootPart")

        if not hrp then 
            return
        end

        for _, child in pairs(tpsPart:GetChildren()) do
            if child:IsA("TouchInterest") then
                local otherPart = child.Part0 or child.Part1
                if otherPart and otherPart:IsA("BasePart") then
                    firetouchinterest(hrp, otherPart, 0)
                    firetouchinterest(hrp, otherPart, 1)
                end
            end
        end
    end,
    Tooltip = 'Instant Contact Event Trigger' 
})

ReactGroup:AddButton({
    Text = ".Touched Event React (Match Ball)",
    Func = function()
        local tpsPart = workspace.TPSSystem and workspace.TPSSystem:FindFirstChild("TPS")
        local radius = 5
        local spamCount = 15

        if not tpsPart then
            return
        end

        -- Bu bir toggle yerine RunService'e bağlı sürekli bir olaydır.
        RunService.Heartbeat:Connect(function()
            local character = getCharacter()
            local hrp = character and character:FindFirstChild("HumanoidRootPart")
            if not hrp then return end

            local function spamTouch(part)
                if not part or (hrp.Position - part.Position).Magnitude > radius then return end

                for _, touching in pairs(part:GetTouchingParts()) do
                    if touching.Parent == character then
                        for _ = 1, spamCount do
                            -- ReplicateSignal kodu burada yer alırdı
                         end
                    end
                end
            end

            spamTouch(tpsPart)
        end)
    end,
    Tooltip = 'Spam Contact Signal (x15)' 
 })

ReactGroup:AddButton({
    Text = "Remove ball delay",
    Func = function()
        local function safeExecute(func)
            local success, err = pcall(func)
            if not success then
            end
        end

        safeExecute(function()
            local tpsPart = workspace.TPSSystem and workspace.TPSSystem:FindFirstChild("TPS")
            if tpsPart then
                for _, item in pairs(tpsPart:GetChildren()) do
                    if item.Name == "APGBDelay" then
                        item:Destroy()
                    end
                end
            end
        end)
    end,
    Tooltip = 'Delete Hit Delay Object' 
})

