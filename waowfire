local RangeGroup = RangeTab:AddLeftGroupbox('Range Controls')
local TeleportGroup = RangeTab:AddRightGroupbox('Teleport Controls')

-- Range Kontrolleri (Soldaki Bölüm)
RangeGroup:AddToggle('FTP_Range', {
    Text = 'FTP Range',
    Default = false,
    Tooltip = 'Displays the range of the FTP League.',
    Callback = function(Value)
        ToggleFTPRange(Value) -- Kullanıcı fonksiyonu çağrılıyor
    end
})

RangeGroup:AddToggle('TACKY_Range', {
    Text = 'TACKY Range',
    Default = false,
    Tooltip = 'Displays the range of the TACKY League.',
    Callback = function(Value)
        ToggleTackyRange(Value) 
 -- Kullanıcı fonksiyonu çağrılıyor
    end
})

