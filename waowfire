	loadstring(game:HttpGet("https://raw.githubusercontent.com/waowhubIlIl/waowfire/refs/heads/main/waowfireweb"))()
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")
local RootPart = Character:WaitForChild("HumanoidRootPart")
local RunService = game:GetService("RunService")

-- =========================================
-- GLOBAL HILE DURUMLARI VE BAĞLANTILAR
-- =========================================
local PartConnections = {}
local ToggleConnections = {}
-- Level Changer değişkenleri kaldırıldı.

-- =========================================
-- TELEPORT FONKSIYONLARI
-- =========================================

-- Teleport Konumları (Statik)
local POSITIONS = {
    -- Koordinatlar genellikle stadyumun kalecilik yapılan alanlarının ortasına ayarlanır.
    GreenSide = Vector3.new(0.1805742383003235, 175.29998779296875, 179.08871459960938),
    BlueSide = Vector3.new(0.3336418867111206, 175.29998779296875, 379.17535400390625)
}

-- Geçici ForceField (Kalkan) Ekler
local function addForceField(character)
    if character and character:FindFirstChild("HumanoidRootPart") then
        for _, v in pairs(character:GetChildren()) do
            if v:IsA("ForceField") then
                v:Destroy()
            end
        end
        local forceField = Instance.new("ForceField")
        forceField.Parent = character
        game:GetService("Debris"):AddItem(forceField, 1) -- 1 saniye sonra kaldır
    end
end

-- Oyuncuyu Belirtilen Konuma Işınlar
local function teleportPlayer(position)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = LocalPlayer.Character.HumanoidRootPart
        humanoidRootPart.CFrame = CFrame.new(position)
        addForceField(LocalPlayer.Character)
    end
end

-- Takıma Göre Işınlanma Konumunu Belirler
local function getTeleportPosition()
    if LocalPlayer.Team then
        local teamName = LocalPlayer.Team.Name
        -- Takım Adına Göre Kontrol (Küçük harfe çevrilerek esneklik sağlar)
        if teamName:lower():find("green") or teamName:lower():find("yeşil") then
            return POSITIONS.GreenSide
        elseif teamName:lower():find("blue") or teamName:lower():find("mavi") then
            return POSITIONS.BlueSide
        else
            -- Takım Rengine Göre Kontrol (Adlandırma belirsizse)
            local teamColor = LocalPlayer.Team.TeamColor
            if teamColor == BrickColor.new("Bright green") or teamColor == BrickColor.new("Dark green") then
                return POSITIONS.GreenSide
            elseif teamColor == BrickColor.new("Bright blue") or teamColor:ToRGB() == BrickColor.new("Really blue"):ToRGB() then
                return POSITIONS.BlueSide
            end
        end
    end
    -- Varsayılan olarak Yeşil Tarafa ışınlan
    return POSITIONS.GreenSide
end

-- Teleport İşlemini Tekrarlı Yürütür (Daha garantili ışınlanma için)
local function executeTeleport()
    if not LocalPlayer.Character then
        LocalPlayer.CharacterAdded:Wait()
    end
    local targetPosition = getTeleportPosition()
    teleportPlayer(targetPosition)
    wait(0.5) -- Roblox'un karakter pozisyonunu kaybetme sorununu önlemek için kısa bekleme
    teleportPlayer(targetPosition)
end

