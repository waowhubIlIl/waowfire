-- General Exploits başlığı FOV olarak değiştirildi
local MiscGroup = MiscTab:AddLeftGroupbox('FOV') 
local UtilityGroup = MiscTab:AddRightGroupbox('Utility Buttons')

-- FOV SLIDER (MiscGroup içinde kaldı)
MiscGroup:AddSlider('FOV', {
    Text = "FOV",
    Default = 70,
    Min = 50,
    Max = 120,
    Rounding = 0, 
    Suffix = "", 
    Tooltip = "Changes your field of view", 
    Callback = function(val)
        if workspace.CurrentCamera then
            workspace.CurrentCamera.FieldOfView = val
        end
    end
})

----------------------------------------
-- UTILITY BUTTONS (Butonlar)
----------------------------------------

-- Anti AFK BUTONU (İsim ve Tooltip güncellendi)
UtilityGroup:AddButton({
    Text = 'Anti AFK', -- (Başlat) kaldırıldı
    Func = function()
        local vu = game:GetService("VirtualUser")
        local player = Players.LocalPlayer

        if not ToggleConnections.AntiAFK then
            ToggleConnections.AntiAFK = player.Idled:Connect(function()
                pcall(function()
                    vu:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
                    task.wait(1)
                    vu:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
                end)
            end)
        else
        end
    end,
    Tooltip = 'Prevents disconnection due to inactivity.' -- Yeni Tooltip
})

-- Anti Offside BUTONU (İsim ve Tooltip güncellendi)
UtilityGroup:AddButton({
    Text = "Anti Offside", -- (Kaldır) kaldırıldı
    Func = function()
        local function safeExecute(func)
            local success, _ = pcall(func)
        end
        safeExecute(function()
            local tpsPart = workspace:FindFirstChild("TPSSystem") and workspace.TPSSystem:FindFirstChild("TPS")
            if tpsPart then
                for _, name in ipairs({ "Offside", "Offside Check" }) do
                    local obj = tpsPart:FindFirstChild(name)
                    if obj then
                        obj:Destroy()
                    end
                end
            end
        end)
    end,
    Tooltip = 'Prevents you from being called offside.' -- Yeni Tooltip
}, "AntiOffside")


----------------------------------------
-- SETTINGS TAB
----------------------------------------
local repo = 'https://raw.githubusercontent.com/violin-suzutsuki/LinoriaLib/main/'
local SaveManager = loadstring(game:HttpGet(repo .. 'addons/SaveManager.lua'))()
local ThemeManager = loadstring(game:HttpGet(repo .. 'addons/ThemeManager.lua'))()

ThemeManager:SetLibrary(Library)
SaveManager:SetLibrary(Library)

local MenuGroup = UISettingsTab:AddLeftGroupbox('Menu Settings')
MenuGroup:AddLabel('Menu Keybind'):AddKeyPicker('MenuKeybind', { 
    Default = 'K', 
    NoUI = true, 
    Text = 'Toggle menu visibility' 
})

Library.ToggleKeybind = Options.MenuKeybind

MenuGroup:AddButton({
    Text = 'Unload Script',
    Func = function()
        -- FOV'u varsayılana geri yükle (70, Roblox varsayılanı)
        if workspace.CurrentCamera then
            workspace.CurrentCamera.FieldOfView = 70 
        end
        -- WalkSpeed'i varsayılana geri yükle (16, Roblox varsayılanı)
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("Humanoid") then
             char.Humanoid.WalkSpeed = 16
           end
        
        -- Range partlarını da unload ederken temizle
        ToggleFTPRange(false)
        ToggleTackyRange(false)
        
        -- Tüm aktif exploit bağlantılarını kes
        for partName, connection in pairs(PartConnections) do
            DisconnectActive(partName)
        end
        
        -- Tüm toggle bağlantılarını kes
        if ToggleConnections.AntiAFK then
            ToggleConnections.AntiAFK:Disconnect()
            ToggleConnections.AntiAFK = nil
        end
        -- Diğer bağlantıları da temizle (Emin olmak için döngü)
        for _, connection in pairs(ToggleConnections) do
            if connection then connection:Disconnect() end
        end

        Library:Unload()
    end,
    Tooltip = 'Completely stops and unloads the script.'
})

ThemeManager:SetFolder('WaowHub_Configs')
SaveManager:SetFolder('WaowHub_Configs')
SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({ 'MenuKeybind' })

ThemeManager:ApplyToTab(UISettingsTab)
SaveManager:BuildConfigSection(UISettingsTab)
SaveManager:LoadAutoloadConfig()
